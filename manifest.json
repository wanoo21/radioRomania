{
	"manifest_version": 2,
	"version": "1.4",
	"name": "Radio Moldova",
	"short_name": "MD radio stations",
	"description": "Asculta online toate posturile Radio din Republica Moldova",
	"author": "IP",
	"homepage_url": "https://github.com/wanoo21/radiosmoldova",
	"offline_enabled": false,
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com; object-src 'self'",
	"browser_action": {
	  "default_icon": {
		   "16": "icons/icon-128.png"
	  },
	  "default_popup": "popup.html",
	  "default_title": "Radio's Moldova"
	},
	"icons": {
		"128": "icons/icon-128.png",
		"48": "icons/icon-48.png",
		"16": "icons/icon-16.png"
	},
	"background": {
		"page": "background.html"
	},
	"update_url": "https://clients2.google.com/service/update2/crx"
}